package interrupt;

public class InterruptMethod_V176 extends Thread {
	public void run() {
		try {
			for (int i = 1; i <= 5; i++) {
				System.out.println("I am a good boy");
				Thread.sleep(1000);
			}
		} catch (InterruptedException e) {
			System.out.println("Invoking the interrupt method");
		}
	}

}
